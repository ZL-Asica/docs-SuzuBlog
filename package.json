{
  "name": "docs-suzublog",
  "version": "1.12.1",
  "private": true,
  "packageManager": "pnpm@10.26.1",
  "scripts": {
    "docs:dev": "vitepress dev docs --port 4000",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 4000",
    "lint": "eslint . --ignore-pattern docs/.vitepress/cache",
    "lint:fix": "eslint --fix . --ignore-pattern docs/.vitepress/cache",
    "commitlint": "commitlint --edit",
    "postinstall": "husky || true"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.1",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint-react/eslint-plugin": "^2.3.13",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.1.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "vitepress": "2.0.0-alpha.15",
    "vue": "^3.5.26"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue,css,json}": [
      "eslint --fix"
    ]
  }
}
